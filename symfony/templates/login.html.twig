{% extends 'base.html.twig' %}
{% block body %}
    <h1>Login!</h1>
    {% if error %}
        <div>{{ error.messageKey|trans(error.messageData, 'security') }}</div>
    {% endif %}
    {{ form_start(form) }}
        {{ form_row(form.username)}}
        {{ form_row(form.password)}}

        <button type="submit">Login</button>
    {{ form_end(form) }}
{% endblock %}
